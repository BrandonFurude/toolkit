# Import the Active Directory module if not already loaded
if (-not (Get-Module -Name ActiveDirectory)) {
    Import-Module ActiveDirectory
}

# Define the CSV file path
$csvFilePath = "csvfile.csv"

# Import the CSV file
$csvData = Import-Csv -Path $csvFilePath

# Initialize an empty array to store the results
$results = @()

# Loop through each user in the CSV file
foreach ($user in $csvData) {
    # Get the user's domain
    $domain = $user.Domain

    # Get the user's domain and username in the format 'Domain\Username'
    $domainUsername = "$domain\$($user.Username)"

    # Get the last logon date of the user from Active Directory
    $lastLogon = (Get-ADUser -Identity $domainUsername -Properties lastLogon).lastLogon

    # Convert the last logon date to a readable format
    $lastLogonDate = [DateTime]::FromFileTime($lastLogon)

    # Add the result to the results array
    $results += New-Object PSObject -Property @{
        Domain = $domain
        Username = $user.Username
        LastLogonDate = $lastLogonDate
    }
}

# Export the results to a new CSV file
$results | Export-Csv -Path "path\to\your\outputfile.csv" -NoTypeInformation

# Display the results in the console
$results | Format-Table -AutoSize
